}


void updateBees() {

    if (collision(bees.screenCol, bees.screenRow, bees.width, bees.height,
        player.screenCol, player.screenRow, player.width, player.height) && cheat == 0) {
            playSoundB(bugBite, 16239, 0);
            livesRemaining = 0;
    }

    animateBees();
}


void animateBees() {
    if (bees.aniCounter % 8 == 0) {
        bees.curFrame = (bees.curFrame + 1) % bees.numFrames;
    }

    bees.aniCounter++;
}


void drawBees() {
    shadowOAM[1].attr0 = bees.screenRow | (0<<14) | (0<<13);
    shadowOAM[1].attr1 = bees.screenCol | (2<<14);
    shadowOAM[1].attr2 = (((bees.curFrame * 4) + 1)*32+((bees.aniState * 4) + 8)) | ((0)<<12);
}


void initRocks() {
    for (int i = 0; i < 2; i++) {
        rocks[i].screenCol = 0;
        rocks[i].screenRow = 117;
        rocks[i].worldCol = 0;
        rocks[i].width = 16;
        rocks[i].height = 16;
        rocks[i].active = 0;
        rocks[i].hit = 0;
    }
}


void updateRocks(ANISPRITE* a) {
    if (a->active) {

        if ((a->screenCol - 1) < -a->width) {
            a->active = 0;
        }


        if (collision(a->screenCol, a->screenRow, a->width, a->height,
            player.screenCol, player.screenRow, player.width, player.height)
            && a->hit == 0
            && cheat == 0) {
                playSoundB(rockHit, 3729, 0);
                player.worldCol -= 35;
                a->hit = 1;
                livesRemaining--;
                hearts[livesRemaining].active = 0;
        }


        a->screenCol = a->worldCol - hOff;
    }


    if (rockTimer == rockTimerEnd) {
        rockTimer = 0;
        rockTimerEnd = rand() % 90 + 10;
        generateRock();
    }
}


void generateRock() {
    for (int i = 0; i < 2; i++) {
        if (!rocks[i].active) {
            rocks[i].worldCol = 240 + hOff;
            rocks[i].screenCol = rocks[i].worldCol;
            rocks[i].active = 1;
            rocks[i].hit = 0;
            break;
        }
    }
}


void drawRocks() {
    for (int i = 0; i < 2; i++) {
        if (rocks[i].active) {
            shadowOAM[2 + i].attr0 = (0xFF & rocks[i].screenRow) | (0<<14) | (0<<13);
            shadowOAM[2 + i].attr1 = (0x1FF & rocks[i].screenCol) | (1<<14);
            shadowOAM[2 + i].attr2 = ((2)*32+(14)) | ((0)<<12);
        } else {
            shadowOAM[2 + i].attr0 = (2<<8);
        }
    }
}


void initBugs() {
    for (int i = 0; i < 3; i++) {
