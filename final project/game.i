void initGame() {
    DMANow(3, spritesheetTiles, &((charblock *)0x6000000)[4], 32768 / 2);
    DMANow(3, spritesheetPal, ((unsigned short *)0x5000200), 256);


    direction = RIGHT;


    livesRemaining = 3;
    bugsCaught = 0;
    usingNet = 0;
    netTimer = 0;


    cheat = 0;
    cheatTimer = 0;


    rockTimer = 0;
    rockTimerEnd = rand() % 90 + 10;


    bugTimer = 0;
    bugTimerEnd = rand() % 270 + 20;


    vOff = 0;
    hOff = 0;


    ground = 100;
    jumping = 1;

    initPlayer();
    initBees();
    initRocks();
    initBugs();
    initHearts();
}

void updateGame() {

    rockTimer++;


    bugTimer++;


    if (usingNet) {
        netTimer++;
    }


    if (cheat == 1) {
        cheatTimer++;
    }


    if (netTimer == 33) {
        usingNet = 0;
        netTimer = 0;
    }


    if (cheatTimer == 600) {
        cheat = 0;
        bugsCaught = 0;
        cheatTimer = 0;
    }


    updatePlayer();


    updateBees();


    for (int i = 0; i < 2; i++) {
        updateRocks(&rocks[i]);
    }


    for (int i = 0; i < 3; i++) {
        updateBugs(&bugs[i]);
    }


    for (int i = 0; i < 3; i++) {
        updateHearts(&hearts[i]);
    }


    if (cheat == 0) {
        hOff += 2;
    } else {
        hOff += 6;
    }
}


void drawGame() {
    drawPlayer();
    drawBees();
    drawRocks();
    drawBugs();
    drawHearts();

    (*(volatile unsigned short *)0x04000014) = hOff;
}


void initPlayer() {
    player.screenRow = 100;
    player.screenCol = 0;
    player.worldRow = ((ground + vOff) << 8);
    player.worldCol = (240 / 2) - (player.width / 2);
    player.rdel = 0;
    player.cdel = 2;
    player.width = 32;
    player.height = 32;
    player.aniCounter = 0;
    player.curFrame = 0;
    player.numFrames = 3;
    player.aniState = RIGHT;
}


void updatePlayer() {


    if (((player.worldRow + player.rdel) >> 8) < ground) {
        player.worldRow += player.rdel;
    } else {
        player.rdel = 0;
        jumping = 0;
    }

    if ((!(~(oldButtons)&((1<<6))) && (~buttons & ((1<<6)))) && !jumping) {
        player.rdel -= 1500;
        jumping = 1;
    }

    player.rdel += 100;


    if (bugsCaught == 3) {
        cheat = 1;
    }


    if ((!(~(oldButtons)&((1<<5))) && (~buttons & ((1<<5)))) && (player.screenCol > 0)) {
        direction = LEFT;
        player.worldCol -= player.cdel;
    }
    if ((~((*(volatile unsigned short *)0x04000130)) & ((1<<5))) && (player.screenCol > 0)) {
        direction = LEFT;
        player.worldCol -= player.cdel;
    }


    if ((!(~(oldButtons)&((1<<4))) && (~buttons & ((1<<4)))) && ((player.screenCol - 1) < 210)) {
        direction = RIGHT;
        player.worldCol += player.cdel;
    }
    if ((~((*(volatile unsigned short *)0x04000130)) & ((1<<4))) && ((player.screenCol - 1) < 210)) {
        direction = RIGHT;
        player.worldCol += player.cdel;
    }


    if ((!(~(oldButtons)&((1<<1))) && (~buttons & ((1<<1)))) && !usingNet && cheat == 0) {
        usingNet = 1;
        player.curFrame = 0;
    }


    player.screenCol = player.worldCol;
    player.screenRow = ((player.worldRow) >> 8) - vOff;

    animatePlayer();
}


void animatePlayer() {
    if (usingNet) {
        if (player.aniCounter % 15 == 0) {
            player.curFrame = (player.curFrame + 1) % player.numFrames;
        }
    } else {
        if (player.aniCounter % 6 == 0) {
            player.curFrame = (player.curFrame + 1) % player.numFrames;
        }
    }


    if ((!(~(oldButtons)&((1<<4))) && (~buttons & ((1<<4))))) {
        player.aniState = RIGHT;
    }
    if ((!(~(oldButtons)&((1<<5))) && (~buttons & ((1<<5))))) {
        player.aniState = LEFT;
    }

    player.aniCounter++;
}


void drawPlayer() {
    if (usingNet == 0) {
        if (cheat == 0) {
            shadowOAM[0].attr0 = (0xFF & player.screenRow) | (0<<14) | (0<<13);
            shadowOAM[0].attr1 = (0x1FF & player.screenCol) | (2<<14);
            shadowOAM[0].attr2 = ((player.curFrame * 4)*32+(player.aniState * 4)) | ((0)<<12);
        } else {
            shadowOAM[0].attr0 = (0xFF & player.screenRow) | (0<<14) | (0<<13);
            shadowOAM[0].attr1 = (0x1FF & player.screenCol) | (2<<14);
            shadowOAM[0].attr2 = ((player.curFrame * 4)*32+((player.aniState * 4) + 16)) | ((0)<<12);
        }
    } else {
        shadowOAM[0].attr0 = player.screenRow | (1<<14) | (0<<13);
        shadowOAM[0].attr1 = (player.screenCol - 16) | (3<<14);
        shadowOAM[0].attr2 = (((player.curFrame * 4) + 12)*32+(player.aniState * 8)) | ((0)<<12);
    }
}


void initBees() {
    bees.screenRow = 95;
    bees.screenCol = 25;
    bees.width = 32;
    bees.height = 32;
    bees.aniCounter = 0;
    bees.curFrame = 0;
