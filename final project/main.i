    state = START;

    seed = 0;
}


void start() {
    seed++;

    waitForVBlank();

    if (bgTimer == 50) {
        if (bgState == 0) {
            DMANow(3, bgStart1Pal, ((unsigned short *)0x5000000), 256);
            DMANow(3, bgStart1Tiles, &((charblock *)0x6000000)[0], 8608 / 2);
            DMANow(3, bgStart1Map, &((screenblock *)0x6000000)[28], 2048 / 2);

