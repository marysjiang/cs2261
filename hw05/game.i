# 5 "game.c" 2


ANISPRITE snake[26];
extern int snakeLength;


ANISPRITE oranges[24];


OBJ_ATTR shadowOAM[128];


enum { UP = 0, DOWN = 1, RIGHT = 2, LEFT = 3};


enum { SNAKEUP = 0, SNAKEDOWN = 1, SNAKERIGHT = 2, SNAKELEFT = 3};
int direction;


int timer;


void initGame() {
    DMANow(3, spritesheetTiles, &((charblock *)0x6000000)[4], 32768 / 2);
    DMANow(3, spritesheetPal, ((unsigned short *)0x5000200), 256);


    direction = SNAKEUP;

    timer = 0;

    initSnake();
    initOranges();
}

void updateGame() {
    updateSnake();

    for (int i = 0; i < 24; i++) {
        updateOranges(&oranges[i]);
    }

    timer++;
}

void drawGame() {
   drawSnake();
   drawOranges();
}


void initSnake() {
    snakeLength = 2;


    for (int i = 0; i < 26; i++) {
        snake[i].row = 72 + (32 * i);
        snake[i].oldRow = snake[i].row;
        snake[i].col = 112;
        snake[i].oldCol = snake[i].col;
        snake[i].rdel = 16;
        snake[i].cdel = 16;
        snake[i].width = 16;
        snake[i].height = 16;
        snake[i].aniCounter = 0;
        snake[i].curFrame = 0;
        snake[i].numFrames = 2;
        snake[i].aniState = UP;
        snake[i].active = 0;
    }


    snake[0].active = 1;
    snake[1].active = 1;
}


void updateSnake() {

    if ((!(~(oldButtons)&((1<<6))) && (~buttons & ((1<<6)))) && direction != SNAKEDOWN) {
        direction = SNAKEUP;
    }

    if ((!(~(oldButtons)&((1<<7))) && (~buttons & ((1<<7)))) && direction != SNAKEUP) {
        direction = SNAKEDOWN;
    }

    if ((!(~(oldButtons)&((1<<4))) && (~buttons & ((1<<4)))) && direction != SNAKELEFT) {
        direction = SNAKERIGHT;
    }

    if ((!(~(oldButtons)&((1<<5))) && (~buttons & ((1<<5)))) && direction != SNAKERIGHT) {
        direction = SNAKELEFT;
    }

    if (timer == 10) {
        for (int i = 0; i < 26; i++) {
            snake[i].oldRow = snake[i].row;
            snake[i].oldCol = snake[i].col;

            if (i == 0) {
                switch (direction) {
                    case SNAKEUP:
                        snake[i].row -= snake[i].rdel;
                        break;
                    case SNAKEDOWN:
                        snake[i].row += snake[i].rdel;
                        break;
                    case SNAKERIGHT:
                        snake[i].col += snake[i].cdel;
                        break;
                    case SNAKELEFT:
                        snake[i].col -= snake[i].cdel;
                        break;
                }
            } else {
                snake[i].row = snake[i - 1].oldRow;
                snake[i].col = snake[i - 1].oldCol;
            }
        }


        if (((snake[0].row - 1) < 0) || (((snake[0].row + snake[0].height) - 1) > 160)
            || ((snake[0].col - 1) < -3) || (((snake[0].col + snake[0].width) - 1) > 240)) {
                snakeLength = 0;
        }


        for (int j = 1; j <= snakeLength; j++) {
            if (collision(snake[0].col, snake[0].row, snake[0].width, snake[0].height,
                snake[j].col, snake[j].row, snake[j].width, snake[j].height)) {
                    snakeLength = 0;
            }
        }


        timer = 0;
    }

    animateSnake();
}


void growSnake() {
    for (int i = 0; i < 26; i++) {
        if (!snake[i].active) {
            snake[i].active = 1;
            snakeLength++;
            break;
        }
    }
}


void animateSnake() {
    if (snake[0].aniCounter % 25 == 0) {
        snake[0].curFrame = (snake[0].curFrame + 1) % snake[0].numFrames;
    }

    for (int i = 0; i < 26; i++) {
        if (i == 0) {
            if ((!(~(oldButtons)&((1<<6))) && (~buttons & ((1<<6)))) && direction != SNAKEDOWN) {
                snake[i].aniState = UP;
            }
            if ((!(~(oldButtons)&((1<<7))) && (~buttons & ((1<<7)))) && direction != SNAKEUP) {
                snake[i].aniState = DOWN;
            }
            if ((!(~(oldButtons)&((1<<4))) && (~buttons & ((1<<4)))) && direction != SNAKELEFT) {
                snake[i].aniState = RIGHT;
            }
            if ((!(~(oldButtons)&((1<<5))) && (~buttons & ((1<<5)))) && direction != SNAKERIGHT) {
                snake[i].aniState = LEFT;
            }
        }
    }

    snake[0].aniCounter++;
}






void drawSnake() {
    for (int i = 0; i < 26; i++) {
        if (snake[i].active) {
            if (i == 0) {
                shadowOAM[i].attr0 = snake[i].row | (0<<14) | (0<<13);
                shadowOAM[i].attr1 = snake[i].col | (1<<14);
                shadowOAM[i].attr2 = ((snake[i].curFrame * 2)*32+(snake[i].aniState * 2)) | ((0)<<12);
            } else {
                shadowOAM[i].attr0 = snake[i].row | (0<<14) | (0<<13);
                shadowOAM[i].attr1 = snake[i].col | (1<<14);
                shadowOAM[i].attr2 = ((12)*32+(0)) | ((0)<<12);
            }
        } else {
            shadowOAM[i].attr0 = (2<<8);
        }
    }
}


void initOranges() {
    for (int i = 0; i < 24; i++) {
        oranges[i].row = (rand() % (140 - 16)) + 16;
        oranges[i].col = (rand() % (220 - 16)) + 16;
        oranges[i].width = 16;
        oranges[i].height = 16;
        oranges[i].aniCounter = 0;
        oranges[i].aniState = 0;
        oranges[i].curFrame = 0;
        oranges[i].numFrames = 4;
        oranges[i].active = 0;
        oranges[i].eaten = 0;
    }

    oranges[0].active = 1;
}


void updateOranges(ANISPRITE* a) {
    if (a->active) {

        if (a->aniCounter % 15 == 0) {
            a->curFrame = (a->curFrame + 1) % a->numFrames;
        }


        if (collision(a->col, a->row, a->width, a->height,
            snake[0].col, snake[0].row, snake[0].width, snake[0].height)) {
                a->active = 0;
                a->eaten = 1;
                growSnake();
                generateOrange();
        }

        a->aniCounter++;
    }
}


void generateOrange() {
    for (int i = 0; i < 24; i++) {
        if (!oranges[i].active && !oranges[i].eaten) {
            oranges[i].active = 1;
            break;
        }
    }
}


void drawOranges() {
    for (int i = 0; i < 24; i++) {
        if (oranges[i].active) {
            shadowOAM[26 + i].attr0 = oranges[i].row | (0<<14) | (0<<13);
            shadowOAM[26 + i].attr1 = oranges[i].col | (1<<14);
            shadowOAM[26 + i].attr2 = (((oranges[i].curFrame * 2) + 4)*32+(oranges[i].aniState * 2)) | ((0)<<12);
        } else {
            shadowOAM[26 + i].attr0 = (2<<8);
        }
    }
}
